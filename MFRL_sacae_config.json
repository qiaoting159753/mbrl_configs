{
    "algorithm": "SACAE",
    "image_observation": 1,
    "G": 1,
    "G_model": 0.2,
    "buffer_size": 1000000,
    "batch_size": 256,
    "num_layers": 4,
    "num_filters": 32,
    "max_steps_exploration": 256,
    "max_steps_training": 1000000,
    "number_steps_per_train_policy": 1,

    "reward_scale": 1.0,
    "actor_lr": 3e-4,
    "critic_lr": 3e-4,
    "alpha_lr": 3e-4,
    "gamma": 0.99,
    "tau": 0.005,

    "encoder_lr": 1e-3,
    "encoder_tau": 0.05,
    "latent_size": 50,

    "min_noise": 0.0,
    "noise_scale": 0.1,
    "noise_decay": 1.0
}

    decoder_lr: Optional[float] = 1e-3
    decoder_latent_lambda: Optional[float] = 1e-6
    decoder_weight_decay: Optional[float] = 1e-7
    decoder_update_freq: Optional[int] = 1

